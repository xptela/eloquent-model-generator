{
    "name": "xptela/eloquent-model-generator",
    "description": "Eloquent Model Generator generates Eloquent models using database schema as a source.",
    "keywords": [
        "xptela",
        "laravel",
        "lumen"
    ],
    "homepage": "https://github.com/xptela",
    "support": {
        "issues": "https://github.com/xptela/eloquent-model-generator/issues",
        "source": "https://github.com/xptela/eloquent-model-generator"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "xptela",
            "email": "xptela@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": ">=2.5",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": ">=1.4",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Xptela\\EloquentModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xptela\\EloquentModelGenerator\\Coders\\CodersServiceProvider"
            ]
        }
    }
}
